<Window x:Class="AnimalApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AnimalApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="208.236" Width="388.546">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="64*"/>
            <ColumnDefinition Width="63*"/>
        </Grid.ColumnDefinitions>
        <Button Content="Dog" HorizontalAlignment="Left" Margin="35,24,0,0" VerticalAlignment="Top" Width="130" Command="{Binding Path=DogClick}" Height="20"/>
        <Button Content="Duck" HorizontalAlignment="Left" Margin="19,24,0,0" VerticalAlignment="Top" Width="130" Command="{Binding Path=DuckClick}" Height="20" Grid.Column="1"/>
        <TextBox HorizontalAlignment="Left" TextWrapping="WrapWithOverflow" Height="100" Margin="19,58,0,0" VerticalAlignment="Top" Width="338" Text="{Binding Path=Text}" Grid.ColumnSpan="2"/>
    </Grid>
</Window>
